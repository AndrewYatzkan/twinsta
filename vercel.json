{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["twitter.js", "gpt3.js"] }
    },
    {
      "src": "public/**",
      "use": "@vercel/static",
      "config": { "includeFiles": ["public/**"] }
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/style.css",
      "dest": "public/style.css"
    },
    {
      "src": "/script.js",
      "dest": "public/script.js"
    },
    {
      "src": "/assets/buildspace-logo.png",
      "dest": "public/assets/buildspace-logo.png"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
